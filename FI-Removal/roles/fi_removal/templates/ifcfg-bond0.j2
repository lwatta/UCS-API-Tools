# Generated by ansible
DEVICE="bond0"
NAME="bond0"
ONBOOT=yes
BOOTPROTO=none
TYPE=Ethernet
USERCTL=no
NM_CONTROLLED=no
IPADDR={{ ansible_default_ipv4.address }}
NETMASK={{ ansible_default_ipv4.netmask }}
GATEWAY={{ ansible_default_ipv4.gateway }}
DOMAIN={{ ansible_domain }}
{% if nameservers and nameservers is defined -%}
{% if nameservers is string -%}
DNS1={{ nameservers }}
{% else -%}
{% for value in nameservers -%}
DNS{{ loop.index }}={{ value }}
{% endfor -%}
{% endif -%}
{% endif -%}
BONDING_OPTS="mode=active-backup miimon=100 primary=eth0"
